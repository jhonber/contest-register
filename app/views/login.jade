extends layout

block content

  a(href = "/auth/google") Log in using your google account

  form(action = "/user/login" , method = "post")
    div
      label Username:
      input(type = "text", name = "username")
    div
      label Password:
      input(type = "password", name = "password")
    div
      input(type = "submit", value = "submit")

